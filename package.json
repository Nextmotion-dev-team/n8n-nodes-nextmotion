{
    "name": "n8n-nodes-nextmotion",
    "version": "0.1.0",
    "description": "n8n community node for NextMotion API integration",
    "license": "MIT",
    "homepage": "",
    "keywords": [
        "n8n-community-node-package"
    ],
    "author": {
        "name": "NextMotion",
        "email": "api@nextmotion.net"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nextmotion/n8n-nodes-nextmotion.git"
    },
   "scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": false,
        "credentials": [
            "dist/credentials/NextMotionApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/NextMotion/NextMotion.node.js",
            "dist/nodes/NextMotion/NextMotionTrigger.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
